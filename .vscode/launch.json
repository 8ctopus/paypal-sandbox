{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "php docker (9000)",
      "type": "php",
      "request": "launch",
      "port": 9000,
      "log": false,
      "stopOnEntry": false
    },
    {
      "name": "php listen CLI",
      "type": "php",
      "port": 9000,
      "stopOnEntry": false,
      "request": "launch"
    },
    {
      // "C:\Program Files\Google\Chrome\Application\chrome.exe" --remote-debugging-port=9222
      "name": "js attach to browser",
      "type": "chrome",
      "request": "attach",
      "url": "http://localhost/",
      "port": 9222,
      "webRoot": "${workspaceFolder}"
    },
    {
      "name": "js launch browser",
      "type": "chrome",
      "request": "launch",
      "url": "http://localhost",
      "port": 9222,
      "webRoot": "${workspaceFolder}"
    }
  ]
}
